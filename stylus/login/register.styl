@import "login.styl"
@import "../issue/issue.styl"
ul
	margin 0
	padding 0
	list-style none
.center_deng
	.deng_num
		position relative
		.get
			width 100px
			height 24px
			border-radius 4px
			font-size 12px
			background #EA5506
			color #FFF
			float right
			position absolute
			top 50%
			margin-top -12px
			right 0px
		.high,.low
			width 12px
			height 8px
			position absolute
			top 16px
			left 97%
			margin-top 5px
			cursor pointer
		.low
			display none
		.menu
			width 60px
			border-radius 4px
			position absolute
			top 32px
			left 84%
			background #A3A3A3
			display none
			.menu_list
				li
					border-bottom 1px solid #FFF
					text-align center
					font-size 12px
					padding 5px
					color #FFF
					&.list_last
						border-bottom 0px
p
	width 100%
	height 14px
	text-align center
	font-size 12px
	display inline-block
	a
		color #EA5506
	
			