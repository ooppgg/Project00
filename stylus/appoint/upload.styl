@import "changedate.styl"
.upload_all
	width 70%
	position absolute
	left 15%
	top 15%
	text-align center
	.up_wor
		font-size 12px
		color #C3C3C4
	.up_pic
		width 100%
		height 143px
		background #878787
		border-radius 5px
		margin-top 10px
		margin-bottom 25px
		text-align center
		position relative
		overflow hidden
		img
			width 45px
			height 45px
			margin-top 45px
		.up_pic_wor
			font-size 12px
			color #FFF
			line-height 20px
		input
			position absolute
			bottom 0px
			right 0px
			font-size 0px
			width 120%
			height 100%
			background rgba(99,99,99,0)
			outline none